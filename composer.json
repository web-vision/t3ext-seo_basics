{
  "name": "web-vision/seo_basics",
  "type": "typo3-cms-extension",
  "description": "Basic SEO features for TYPO3",
  "homepage": "https://github.com/b13/t3ext-seo_basics",
  "license": ["GPL-2.0-or-later"],
  "keywords": ["TYPO3 CMS", "SEO Basics"],
  "support": {
    "issues": "https://github.com/b13/t3ext-seo_basics/issues"
  },
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4"
  },
  "autoload": {
    "psr-4": {
      "B13\\SeoBasics\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/seo-basics": "*"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "seo_basics"
    }
  },
  "suggest": {
    "helhum/realurl": "*",
    "typo3/cms-seo": "^9.5.0"
  }
}
